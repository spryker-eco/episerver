<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd" >

    <transfer name="Mail">
        <property name="optivoMail" type="OptivoMail" />
    </transfer>

    <transfer name="OptivoMail">
        <property name="mailingListToken" type="string" />
        <property name="recepientId" type="string" />
        <property name="mailingId" type="string" />
        <property name="personalizedAttachmentsToken" type="string" />
        <property name="sanitize" type="bool" />

        <property name="subscriber" type="OptivoSubscriber" />
    </transfer>

    <transfer name="OptivoSubscribeRequest">
        <property name="recepientId" type="string" />
        <property name="optInId" type="string" />
        <property name="optInFrequencyLimit" type="int" />
        <property name="overwrite" type="bool" />
        <property name="optInSource" type="string" />
        <property name="sanitize" type="bool" />

        <property name="subscriber" type="OptivoSubscriber" />
    </transfer>

    <transfer name="OptivoResponse">
        <property name="status" type="string" />
    </transfer>

    <transfer name="OptivoSubscriber">
    </transfer>

</transfers>
